{
  "name": "template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "generate": "node scripts/generate",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ./data-source.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create ./src/database/migrations/Migration",
    "migration:generate": "yarn build && yarn typeorm migration:generate ./src/database/migrations/Migration",
    "migration:run": "yarn build && yarn typeorm migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "@aws-sdk/client-s3": "^3.523.0",
    "@aws-sdk/client-sns": "^3.521.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "aws-sdk": "^2.1562.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^8.2.0",
    "graphql": "^16.6.0",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "module-alias": "^2.2.2",
    "ms": "^3.0.0-canary.1",
    "mysql2": "^3.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.4",
    "solapi": "^5.3.0",
    "ts-dedent": "^2.2.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@ngrok/ngrok": "^1.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/csvtojson": "^2.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "4.17.13",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.17",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^29.7.0",
    "prettier": "2.5.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
